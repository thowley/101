#include "stdio.h"
#include "math.h"
int main(void) {
	char input;
	int ascii;
	double squareR;
	int prec;


	fprintf(stderr, "Enter any character from the keyboard.\n");
	scanf("%c",&input);


	ascii = (int)input;
	squareR = sqrt(ascii);
	printf("The ASCII value is: %d.\n ",input);

	printf("The square root value of '%c' is : %0.1f.\n",input,squareR);
	
	prec = (int)squareR;

	printf("The value of PI is : %.*f.\n",prec, M_PI);

	return 0;
}




